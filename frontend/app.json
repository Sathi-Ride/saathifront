{
  "expo": {
    "name": "Saathi",
    "slug": "Saathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "Saathi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to show your current position on the map and find nearby rides.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to track your position for ride sharing."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDbYiu_14LlULrCl6WXSNvTgEy3yBCKkQg"
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDbYiu_14LlULrCl6WXSNvTgEy3yBCKkQg"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.prameshk7.frontend",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show your position on the map and find nearby rides."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f9a54f7e-1f52-4ebf-8463-421f4fe29ff3"
      },
      "API_URL": "http://192.168.0.8:9000/api/v1",
      "WEBSOCKET_URL": "http://192.168.0.8:9000",
      "PUBLIC_ASSET_URL": "http://192.168.0.8:9000",
      "DEFAULT_BASE_URL": "http://localhost:3000",
      "GOOGLE_MAPS_API_KEY": "AIzaSyDbYiu_14LlULrCl6WXSNvTgEy3yBCKkQg",
      "SUPPORT_PHONE": "+977980010100",
      "SUPPORT_EMAIL": "support@saathiapp.com",
      "FACEBOOK_URL": "https://www.facebook.com/saathiapp",
      "INSTAGRAM_URL": "https://www.instagram.com/saathiapp",
      "TWITTER_URL": "https://twitter.com/saathiapp",
      "WEBSITE_URL": "https://www.saathiapp.com"
    }
  }
}
